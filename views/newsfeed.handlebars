<div class="container blog">
    <div class="row">
        <div class="col-md-12 newsfeed-container">
            <ul>
                {{#each recommends}}
                <div class = "newscard">
                <p><span class="label">Movie Title:</span> {{ movieTitle }}</p>
                <p><span class="label">Recommendation:</span> {{ recommendation }}</p>
                <p><span class="label">Username:</span> {{ userName }}</p>
                </div>
                {{/each}}
            </ul>
        </div>
    </div>
</div>

    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.0/moment.min.js" type="text/javascript"></script>
    